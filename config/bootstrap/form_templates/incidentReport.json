        "tabulatorList": {
            "enabled": true,
            "dataTransform": "javascript: const serviceLabels = { fmBroadcast: 'FM Broadcast', liveStream: 'Live Stream', studio: 'Studio Equipment', itNetwork: 'IT / Network', powerHvac: 'Power / HVAC' }; return (submissions || []).map(sub => { const data = sub.data || {}; const services = []; if (data.affectedServices) { Object.entries(data.affectedServices).forEach(([key, value]) => { if (value === true) { services.push(serviceLabels[key] || key); } }); } return { ...sub, affectedServicesDisplay: services.join(', ') }; });",
            "initialSort": [
            {
                "column": "data.incidentDate",
                "dir": "desc"
            }
            ],
            "layout": "fitColumns",
            "paginationSize": 20,
            "responsiveLayout": true,
            "columns": [
